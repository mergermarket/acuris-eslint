{
  "name": "@acuris/eslint-config",
  "version": "0.0.66",
  "private": false,
  "description": "Acuris eslint shareable configuration",
  "keywords": [
    "acuris",
    "eslint",
    "prettier"
  ],
  "license": "MIT",
  "author": "Acuris, Salvatore Previti",
  "homepage": "https://github.com/mergermarket/acuris-eslint#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mergermarket/acuris-eslint.git"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "main": "index.js",
  "bin": {
    "acuris-eslint": "bin/acuris-eslint.js"
  },
  "scripts": {
    "lint": "node scripts/acuris-eslint.js",
    "help": "node scripts/acuris-eslint.js --help",
    "acuris-eslint": "node scripts/acuris-eslint.js",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "mocha \"test/**/*.test.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx}": [
      "node scripts/acuris-eslint.js --lint-staged --fix --max-warnings=0"
    ]
  },
  "bundledDependencies": [
    "comment-json",
    "eslint-plugin-quick-prettier",
    "v8-compile-cache"
  ],
  "peerDependencies": {
    "eslint": ">=6.8.0",
    "eslint-plugin-import": ">=2.20.2",
    "eslint-plugin-json": ">=2.1.1",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-promise": ">=4.2.1",
    "prettier": ">=2.0.5"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "comment-json": "^3.0.2",
    "eslint-plugin-quick-prettier": "0.0.28",
    "glob-intersection": "^0.1.3",
    "semver": "^7.3.2",
    "v8-compile-cache": "2.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-parcel": "^1.10.4",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-scope": "^5.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "mocha": "^7.1.2",
    "mock-fs": "^4.12.0",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  }
}
