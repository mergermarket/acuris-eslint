{
  "name": "@acuris/eslint-config",
  "version": "0.1.14",
  "private": false,
  "description": "Acuris eslint shareable configuration",
  "keywords": [
    "acuris-eslint",
    "acuris",
    "eslint",
    "prettier"
  ],
  "license": "MIT",
  "author": "Acuris, Salvatore Previti",
  "homepage": "https://github.com/mergermarket/acuris-eslint#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mergermarket/acuris-eslint.git"
  },
  "engines": {
    "node": ">=12.12.0"
  },
  "main": "index.js",
  "bin": {
    "acuris-eslint": "bin/acuris-eslint.js"
  },
  "scripts": {
    "lint": "node scripts/acuris-eslint.js",
    "help": "node scripts/acuris-eslint.js --help",
    "acuris-eslint": "node scripts/acuris-eslint.js",
    "postinstall": "node ./scripts/postinstall.js",
    "test": "mocha \"test/**/*.test.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*": [
      "acuris-eslint --lint-staged --fix --max-warnings=0"
    ]
  },
  "bundleDependencies": [
    "chalk",
    "comment-json",
    "eslint-plugin-quick-prettier",
    "ignore",
    "semver",
    "v8-compile-cache"
  ],
  "peerDependencies": {
    "eslint": ">=7.11.0",
    "eslint-plugin-import": ">=2.22.1",
    "eslint-plugin-json": ">=2.1.2",
    "eslint-plugin-node": ">=11.1.0",
    "eslint-plugin-promise": ">=4.2.1",
    "prettier": ">=2.1.2"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "comment-json": "^4.1.0",
    "eslint-plugin-quick-prettier": "0.0.29",
    "ignore": "^5.1.8",
    "semver": "^7.3.2",
    "v8-compile-cache": "2.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.14.2",
    "@types/prettier": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.11.0",
    "eslint-import-resolver-parcel": "^1.10.5",
    "eslint-plugin-chai-expect": "^2.2.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-scope": "^5.1.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.4.2",
    "mocha": "^8.2.0",
    "mock-fs": "^4.13.0",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
